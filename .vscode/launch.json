  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug swaggie",
      "program": "${workspaceFolder}/src/cli.ts",
      "preLaunchTask": "Build",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "args": [
        "-s",
        "https://petstore.swagger.io/v2/swagger.json",
        "-o",
        "./swagger-test/",
        "-r"
      ]
    }
  ]
}
