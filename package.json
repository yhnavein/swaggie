{
  "name": "swaggie",
  "version": "0.1.3",
  "description": "Generate ES6 or TypeScript service integration code from an OpenAPI spec",
  "author": {
    "name": "Piotr Dabrowski",
    "url": "https://github.com/yhnavein"
  },
  "license": "MIT",
  "homepage": "https://github.com/yhnavein/swaggie",
  "repository": {
    "type": "git",
    "url": "https://github.com/yhnavein/swaggie.git"
  },
  "bugs": {
    "url": "https://github.com/yhnavein/swaggie/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/index.js",
  "bin": {
    "swaggie": "dist/cli.js"
  },
  "scripts": {
    "start": "ts-node ./src/cli.ts",
    "prebuild": "npm run init",
    "build": "tsc",
    "predev": "npm run init",
    "dev": "tsc -w",
    "init": "npm run clean",
    "clean": "shx rm -rf ./dist"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "swagger",
    "swagger 2.0",
    "openapi",
    "rest",
    "service",
    "typescript",
    "codegen"
  ],
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "2.19.0",
    "ejs": "^2.6.1",
    "got": "9.6.0",
    "inversify": "^5.0.1",
    "js-yaml": "3.13.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "prettier": "1.16.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/ejs": "2.6.3",
    "@types/expect": "1.20.4",
    "@types/got": "9.4.1",
    "@types/js-yaml": "3.12.0",
    "@types/lodash": "4.14.123",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "5.2.6",
    "@types/node": "11.11.6",
    "@types/prettier": "1.16.1",
    "expect": "24.5.0",
    "mocha": "6.0.2",
    "shx": "^0.3.2",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "typescript": "3.3.4000"
  }
}
