{
  "name": "swaggie",
  "version": "1.6.0",
  "description": "Generate TypeScript REST client code from an OpenAPI spec",
  "author": {
    "name": "Piotr Dabrowski",
    "url": "https://github.com/yhnavein"
  },
  "license": "MIT",
  "homepage": "https://github.com/yhnavein/swaggie",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhnavein/swaggie.git"
  },
  "bugs": {
    "url": "https://github.com/yhnavein/swaggie/issues"
  },
  "engines": {
    "node": ">=18.18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "swaggie": "dist/cli.js"
  },
  "scripts": {
    "build": "sucrase ./src -d ./dist --transforms typescript,imports && bun run rm-tests && bun run types",
    "rm-tests": "find dist/ \\( -name '*.spec.js' -o -name 'types.js' \\) -type f -delete",
    "types": "tsc src/types.ts --outDir dist/ --declaration --emitDeclarationOnly && cp test/index.d.ts ./dist/"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "swagger",
    "openapi",
    "openapi 3.0",
    "rest",
    "rest client",
    "fetch",
    "axios",
    "angular",
    "xior",
    "swr",
    "service",
    "typescript",
    "codegen",
    "TanStack Query"
  ],
  "dependencies": {
    "case": "^1.6.3",
    "commander": "^14.0.3",
    "eta": "^4.5.1",
    "yaml": "^2.8.2",
    "nanocolors": "^0.2.0"
  },
  "devDependencies": {
    "bun-types": "1.3.9",
    "openapi-types": "^12.1.3",
    "sucrase": "3.35.1",
    "typescript": "5.9.3"
  }
}
